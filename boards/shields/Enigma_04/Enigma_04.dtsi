#include "Enigma_04_layouts.dtsi"
/ {
    chosen {
        zmk,kscan = &kscan_0;
        zmk,physical-layout = &layout_0;
        zmk,studio-rpc = &studio_usb;
    };
  
    transform_0: transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <10>;
        rows = <3>;               
        map = <
                    RC(0,1) RC(0,2) RC(0,3) RC(0,4)     RC(0,9) RC(0,8) RC(0,7) RC(0,6)
            RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4)     RC(1,9) RC(1,8) RC(1,7) RC(1,6) RC(1,5)
            RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4)     RC(2,9) RC(2,8) RC(2,7) RC(2,6) RC(2,5)
        >;
    };

    transform_1: transform_1 {
        compatible = "zmk,matrix-transform";
        columns = <10>;
        rows = <3>;               
        map = <
                    RC(0,1) RC(0,2) RC(0,3) RC(0,4)       RC(0,9) RC(0,8) RC(0,7) RC(0,6)
            RC(3,0) RC(1,0) RC(1,2) RC(1,3) RC(1,4)       RC(1,9) RC(1,8) RC(1,7) RC(1,5) RC(3,5)
            RC(3,1) RC(2,0) RC(2,1) RC(2,3) RC(2,4)       RC(2,9) RC(2,8) RC(2,6) RC(2,5) RC(3,6)
        >;
    };

    kscan_0: kscan_0 {
        compatible = "zmk,kscan-gpio-matrix";
        diode-direction = "row2col";
            row-gpios
                = <&gpio (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>,
                  <&gpio (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>,
                  <&gpio (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;
    };

    studio_usb: studio_usb {
        compatible = "zmk,studio-rpc-usb-uart";
    };
};
